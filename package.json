{
  "name": "wahtson-electron",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "lint": "prettier --single-quote --trailing-comma all --no-semi --arrow-parens avoid --print-width 100 --tab-width 4 --write src/**/*.js",
    "rebuild": "electron-rebuild",
    "build-win": "electron-rebuild && electron-packager . wahtson-electron --overwrite --asar=true --platform=win32 --arch=x64 --icon=assets/icon.ico --prune=true --out=builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Wahtson Electron\" --ignore=\"(.gitignore)|(config.toml)|(database.sqlite)|(builds)\""
  },
  "dependencies": {
    "mz": "^2.7.0",
    "open": "^7.0.3",
    "package": "^1.0.1",
    "toml": "^3.0.0",
    "wahtson": "^2.1.0"
  },
  "devDependencies": {
    "electron": "^9.0.0",
    "electron-packager": "^14.2.1",
    "electron-rebuild": "^1.11.0",
    "prettier": "^2.0.5"
  },
  "engines": {
    "node": ">=12"
  },
  "author": "",
  "license": "GPL-3.0",
  "description": ""
}
